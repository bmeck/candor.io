print = global.print
Timer = global.uv.Timer

timer = Timer.create()
timer2 = Timer.create()

Timer.start(timer, 2400, 0, () {
  print("onTimeout")
  Timer.stop(timer2)
  Timer.stop(timer)
  Timer.close(timer, () {
    print("close1")
  })
  Timer.close(timer2, () {
    print("close2")
  })
})

Timer.start(timer2, 333, 333, () {
  Timer = global.uv.Timer
  print("onInterval")
  period = Timer.getRepeat(timer2)
  print("period", period)
  Timer.setRepeat(timer2, period / 1.2 + 1)
})

print("timer", timer)
print("timer2", timer2)
